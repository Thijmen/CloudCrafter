{
  "name": "plugins-codegen",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/plugins/codegen/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/plugins/codegen",
        "main": "libs/plugins/codegen/src/index.ts",
        "tsConfig": "libs/plugins/codegen/tsconfig.lib.json",
        "assets": ["libs/plugins/codegen/*.md",
          {
            "glob": "executors.json",
            "input": "./libs/plugins/codegen",
            "output": "."
          }
        ]
      }
    }
  }
}
